@article{kaspi_chandra_2001,
	title = {Chandra {X}-{Ray} {Observations} of {G11}.2-0.3: {Implications} for {Pulsar} {Ages}},
	volume = {560},
	issn = {0004-637X},
	shorttitle = {Chandra {X}-{Ray} {Observations} of {G11}.2-0.3},
	url = {https://ui.adsabs.harvard.edu/abs/2001ApJ...560..371K},
	doi = {10.1086/322515},
	abstract = {We present Chandra X-Ray Observatory imaging observations of the young Galactic supernova remnant G11.2-0.3. The image shows that the previously known young 65 ms X-ray pulsar is at position (J2000) R.A. 18h11m29.22s, decl. -19°25'27.6", with 1 σ error radius of 0.6". This is within 8" of the geometric center of the shell. This provides strong confirming evidence that the system is younger, by a factor of {\textasciitilde}12, than the characteristic age of the pulsar. The age discrepancy suggests that pulsar characteristic ages can be poor age estimators for young pulsars. Assuming conventional spin-down with constant magnetic field and braking index, the most likely explanation for the age discrepancy in G11.2-0.3 is that the pulsar was born with a spin period of {\textasciitilde}62 ms. The Chandra image also reveals, for the first time, the morphology of the pulsar wind nebula. The elongated hard X-ray structure can be interpreted as either a jet or a Crab-like torus seen edge-on. This adds to the growing list of highly aspherical pulsar wind nebulae and argues that such structures are common around young pulsars.},
	urldate = {2024-01-19},
	journal = {The Astrophysical Journal},
	author = {Kaspi, V. M. and Roberts, M. E. and Vasisht, G. and Gotthelf, E. V. and Pivovaroff, M. and Kawai, N.},
	month = oct,
	year = {2001},
	note = {ADS Bibcode: 2001ApJ...560..371K},
	keywords = {Astrophysics, PSR J1811-1925), pulsars: individual (AX J1811.5-1926, Stars: Pulsars: General, supernovae: individual (G11.2-0.3), X-Rays: General},
	pages = {371--377},
	file = {Full Text PDF:/Users/oj/Zotero/storage/LWD7TT6K/Kaspi et al. - 2001 - Chandra X-Ray Observations of G11.2-0.3 Implicati.pdf:application/pdf},
